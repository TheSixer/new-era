{
  "name": "@wmeimob/taro-dev-tools",
  "version": "1.0.0",
  "description": "taro 开发调试工具",
  "scripts": {
    "dev": "cross-env REACT_APP_ENV=dev npm run build:weapp -- --watch",
    "uat": "cross-env REACT_APP_ENV=uat npm run build:weapp",
    "stg": "cross-env REACT_APP_ENV=stg npm run build:weapp",
    "build": "cross-env REACT_APP_ENV=production npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "dev:h5": "cross-env REACT_APP_ENV=dev npm run build:h51 -- --watch --env development",
    "uat:h5": "cross-env REACT_APP_ENV=uat npm run build:h51",
    "stg:h5": "cross-env REACT_APP_ENV=stg npm run build:h51",
    "build:h5": "cross-env REACT_APP_ENV=production npm run build:h51",
    "build:h51": "taro build --type h5",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@wmeimob/taro-design": "workspace:^1.5.1",
    "@wmeimob/utils": "workspace:^1.4.0"
  },
  "peerDependencies": {
    "@babel/runtime": "7.21.0",
    "@tarojs/api": "3.5.11",
    "@tarojs/cli": "3.5.11",
