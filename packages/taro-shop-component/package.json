{
  "name": "@wmeimob/taro-shop-component",
  "version": "2.4.0",
  "description": "商城页面组件",
  "private": true,
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "//1": "小程序构建命令",
    "dev": "npm run dev:weapp",
    "dev:weapp": "cross-env REACT_APP_ENV=dev npm run build:weapp -- --watch",
    "build:dev:weapp": "cross-env REACT_APP_ENV=dev npm run build:weapp -- --mode development",
    "build:uat:weapp": "cross-env REACT_APP_ENV=uat npm run build:weapp -- --mode uat",
    "build:prd:weapp": "cross-env REACT_APP_ENV=prd npm run build:weapp",
    "//2": "h5构建命令",
    "dev:h5": "cross-env REACT_APP_ENV=dev npm run build:h5 -- --watch --host localhost",
    "build:dev:h5": "cross-env REACT_APP_ENV=dev npm run build:h5 -- --mode development",
    "build:uat:h5": "cross-env REACT_APP_ENV=uat npm run build:h5 -- --mode uat",
    "build:prd:h5": "cross-env REACT_APP_ENV=prd npm run build:h5",
    "//3": "其他命令",
    "preview:dev": "cross-env NODE_ENV=production REACT_APP_ENV=dev npm run build:weapp -- --watch",
    "build": "cross-env REACT_APP_ENV=prd npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "api": "mmapi a"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
